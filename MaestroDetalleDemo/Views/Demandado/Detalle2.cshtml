        <td>
            @Html.DisplayFor(modelItem => item.COD_DADO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NOM_DADO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.APEPAT_DADO)
