@model IEnumerable<MaestroDetalleDemo.Models.TB_DEMANDADO_PERSONA>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.COD_DADO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOM_DADO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.APEPAT_DADO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.APEMAT_DADO)
        </th>
