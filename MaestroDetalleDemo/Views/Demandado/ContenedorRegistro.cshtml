<script type="text/javascript">

    $(document).ready(function () {
        // Smart Wizard
        $('#wizard').smartWizard({ transitionEffect: 'slide' });

    });
</script>

<table align="center" border="0" cellpadding="0" cellspacing="0">
    <tr>
        <td>
